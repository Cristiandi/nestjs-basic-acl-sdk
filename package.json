{
  "name": "nestjs-basic-acl-sdk",
  "version": "0.4.3",
  "description": "sdk for basic acl api",
  "author": "Cristiandi <cristiandavidippolito@gmail.com>",
  "homepage": "https://github.com/Cristiandi/nestjs-basic-acl-sdk",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "TypeScript",
    "Access Control List",
    "GraphQL"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cristiandi/nestjs-basic-acl-sdk.git"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/Cristiandi/nestjs-basic-acl-sdk/issues"
  },
  "dependencies": {
    "graphql": "16.7.1",
    "graphql-request": "6.1.0",
    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-express": "10.1.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.12",
    "@nestjs/graphql": "12.0.8",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "18.17.5",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@apollo/server": "4.9.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "jest",
    "prettier": "3.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}